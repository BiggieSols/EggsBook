<div class="user">
  <br><br>


  <div class="row">
    <div class="col-xs-4 left-col">
      <div class="profile-picture">
        <img src="<%= user.get('photo_urls').medium %>" class="thumbnail">
      </div>

          <div class="user-name">
            <%= user.get('name') %>
        </div>

          <div class="friend-button">
          </div>
      <br>
      <br>

      <div class="favorite-foods-heading">
        Breakfast Selections
      </div>
      <% user.get('favorite_food_ids').forEach(function(food_id) { %>
      <% var food = foods.get(food_id) %>
      <% if(food) { %>
      <img src="<%= food.get('photo_urls').medium %>" class="profile-food-photo thumbnail">
      <% } %>
      <% }); %>
    </div>
    <div class="col-xs-8">







      <nav class="navbar navbar-default navbar-static-top" role="navigation">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#user-profile-nav">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
        </div>

        <!-- Collect the nav links, forms, and other content for toggling -->
        <div class="collapse navbar-collapse" id="user-profile-nav">
          <ul class="nav nav-pills">
            <li id="render-posts" class="active"><a>Posts</a></li>
            <li id="render-photos"><a>Photos</a></li>
            <li id="render-friends"><a>Friends</a></li>
          </ul>
        </div><!-- /.navbar-collapse -->
      </nav>


      <div id="profile-content">
      </div>
    </div>
  </div>
</div>