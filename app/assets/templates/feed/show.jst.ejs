<br><br>
<h1>Write a Post</h1>

<form enctype="multipart/form-data">
  <label for="post_details">Post</label>
  <input type="text"
          name="post[details]"
          id="post_details">

  <input type="file"
         name="post[image]"
         accept="image/*">

  <button id="new-post">Create Post</button>
</form>

<h1>Your Feed</h1>

<div id="feed-posts">
  <% feed.models.forEach(function(post) { %>
    <div class="post-container" data-id="<%= post.id %>" data-type="post"%>
      post goes here
    </div>
  <% }); %>
</div>